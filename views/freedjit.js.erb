(function(id, key) {
    $.get(
        "http://<%= @server %>/visit",
        { key: key, title: document.title, url: document.URL },
        function(data) {},
        "jsonp");
    var list = function() {
        $.get(
            "http://<%= @server %>/list",
            { key: key },
            function(response) {
                $(id).html(response.visits);
            },
            "jsonp");
    };
    list();
    setInterval(list, 60000);
})('#list', '<%= @key %>')
