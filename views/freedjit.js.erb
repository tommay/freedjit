(function (key, server) {
    var a = $(".post-title a");
    $.get(
        "http://" + server + "/visit",
        {
            key: key,
            page: document.URL,
            title: (a && a.html()) || document.title,
            url: (a && a.attr("href")) || "",
            ref: document.referrer || ""
        },
        function (response) {
            response.apply(key, server);
        },
        "jsonp");
}('<%= @key %>', '<%= @server %>'));
