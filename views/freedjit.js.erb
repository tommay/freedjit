(function (key, server) {
    var a = $(".post-title a");
    $.get(
        "http://" + server + "/visit",
        {
            key: key,
            title: document.title,
            url: document.URL,
            ref: document.referrer || "",
            t: a.html() || "",
            h: a.attr("href") || ""
        },
        function (response) {
            response.apply(key, server);
        },
        "jsonp");
}('<%= @key %>', '<%= @server %>'));
