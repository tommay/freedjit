(function (key, http_root) {
    var a = $(".post-title a");
    $.get(
        http_root + "visit",
        {
            key: key,
            page: document.URL,
            title: (a && a.html()) || document.title,
            url: (a && a.attr("href")) || "",
            ref: document.referrer || ""
        },
        function (response) {
            response.apply(key, http_root);
        },
        "jsonp");
}('<%= key %>', '<%= http_root %>'));
