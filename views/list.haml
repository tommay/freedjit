%ul.visits
  - @list.each do |visit|
    %li
      A #{visit.new? ? "new" : "returning"} visitor
      - if visit.where
        from
        %span.visit-where= visit.where
      viewed
      - if visit.url
        %a.visit-link{:href => visit.url}= visit.title || "a page"
      - else
        = visit.title || "a page"
      = visit.when
