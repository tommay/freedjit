{
<% if ok %>
    apply: function (key, http_root) {
        (function (list) {
            list();
            setInterval(list, 60000);
        }(function () {
            $.get(
                http_root + "list",
                { key: key },
                function (response) {
                    response.apply();
                },
                "jsonp"
            );
        }));
    }
<% else %>
    apply: function (key, http_root) {}
<% end %>
}
